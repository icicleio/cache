{
    "name": "icicleio/cache",
    "description": "Asynchronous caching interface for Icicle.",
    "keywords": [
        "cache",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Cache\\": "tests"
        }
    }
}
